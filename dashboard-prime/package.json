{
  "name": "dashboard-prime",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npm run setup:common",
    "dev": "vite",
    "build": "npm install && npm run setup:common && npm run licenseCheck && npm run getDashboardVersion && vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "setup:common": "npx symlink-dir ../common-components/src/common src/common-components",
    "getDashboardVersion": "node ../scripts/getDashboardVersion.js",
    "licenseDepCheck": "license-checker --excludePrivatePackages --production --onlyAllow='MIT;ISC;Apache-2.0;BSD-2-Clause;BSD-3-Clause;Unlicense;Custom: https://travis-ci.org/component/emitter.png' --summary",
    "licenseCheck": "npm run licenseDepCheck",
    "removeServiceResources": "rm -rf ../service/src/main/resources/public/assets && rm -rf ../service/src/main/resources/public/themes && rm ../service/src/main/resources/public/index.html && rm ../service/src/main/resources/public/skilltree.ico",
    "copyDistToService": "cp -rT dist ../service/src/main/resources/public/ && cp -rT dist ../service/target/classes/public/",
    "deploy": "npm install && npm run build && npm run copyDistToService"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.4",
    "@skilltree/skills-client-js": "3.5.0",
    "@toast-ui/editor": "3.2.2",
    "@vue-a11y/announcer": "3.1.5",
    "@vueuse/core": "10.7.2",
    "axios": "1.6.5",
    "dayjs": "1.11.10",
    "deep-equal": "2.2.3",
    "dompurify": "3.0.8",
    "enquire.js": "2.1.6",
    "idb-keyval": "6.2.1",
    "node-emoji": "2.1.3",
    "pinia": "2.1.7",
    "primeflex": "3.3.1",
    "primeicons": "6.0.1",
    "primevue": "3.46.0",
    "sortablejs": "1.15.2",
    "tui-editor-plugin-font-size": "1.0.4",
    "vee-validate": "4.12.4",
    "vue": "3.4.11",
    "vue-router": "4.2.5",
    "vuex": "4.1.0",
    "yup": "1.3.3"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.6.1",
    "@vitejs/plugin-vue": "5.0.3",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/test-utils": "2.4.3",
    "eslint": "8.56.0",
    "eslint-plugin-vue": "9.20.0",
    "jsdom": "23.2.0",
    "license-checker": "25.0.1",
    "prettier": "3.2.1",
    "symlink-dir": "5.2.1",
    "vite": "5.1.2",
    "vitest": "1.2.0"
  }
}
